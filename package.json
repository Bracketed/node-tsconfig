{
	"name": "@bracketed/typescript-config",
	"description": "Typescript Config files to make coding easier!",
	"packageManager": "yarn@4.9.2",
	"version": "4.7.1",
	"publishConfig": {
		"access": "public"
	},
	"license": "Apache-2.0",
	"author": "@bracketed",
	"main": "./lib/tsconfig.json",
	"exports": {
		"./*": "./lib/*.json"
	},
	"files": [
		"lib"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Bracketed/node-tsconfig"
	},
	"homepage": "https://bracketed.co.uk",
	"sideEffects": false,
	"bugs": {
		"url": "https://github.com/Bracketed/node-tsconfig/issues"
	},
	"keywords": [
		"bracketed",
		"typescript",
		"ts",
		"yarn",
		"typescript config",
		"tsconfig"
	],
	"scripts": {
		"lint": "yarn prettier --write .",
		"validate": "yarn prettier --check .",
		"build": "tsup",
		"update": "yarn upgrade-interactive"
	},
	"devDependencies": {
		"prettier": "^3.5.3",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=18"
	}
}
